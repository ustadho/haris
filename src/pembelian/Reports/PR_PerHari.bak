	<queryString><![CDATA[select prd.no_pr, pr.release_date::date as release_date, coalesce(pr.requested_by,'') as req_by,  pr.site_id, prd.kode_barang, 
coalesce(i.nama_paten,'') as nama_barang, i.satuan_kecil, prd.jumlah
from phar_pr_detail prd
inner join phar_pr pr on pr.no_pr=prd.no_pr
left join barang i on i.item_code=prd.kode_barang
where to_char(pr.release_date, 'yyyy-MM-dd')>=$P{tanggal1}
and to_char(pr.release_date, 'yyyy-MM-dd')<=$P{tanggal2}
and coalesce(pr.site_id,'') like $P{site}||'%'
order by pr.release_date, prd.no_pr, coalesce(i.nama_paten,'')]]></queryString>

