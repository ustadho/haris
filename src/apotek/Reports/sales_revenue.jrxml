	<queryString>
		<![CDATA[select tanggal, sum(coalesce(keluar,0)*coalesce(hpp,0)) as pokok,
sum(coalesce(keluar,0)*coalesce(harga_sat,0)) as penjualan,
sum(coalesce(keluar,0)*(coalesce(harga_sat,0)-coalesce(hpp,0))) as revenue
from r_kartu_stok  where tipe='JL'
and to_char(tanggal, 'yyyy-MM-dd')>= $P{tanggal1}
and to_char(tanggal, 'yyyy-MM-dd')<=$P{tanggal2}
group by tanggal
order by tanggal]]>
	</queryString>
