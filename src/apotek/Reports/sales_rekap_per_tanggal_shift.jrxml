	<queryString>
		<![CDATA[select tanggal::date, case when shift='P' then 'Pagi' else 'Siang' end as shift,
sum(coalesce(tunai,0)) as bayar, sum(coalesce(kredit,0)) as kredit,
sum(coalesce(total,0)) as total  from fn_rpt_penjualan_rekap_shift($P{tanggal1}, $P{tanggal2},
$P{shift}, $P{jenis}) as (no_penjualan varchar, tanggal timestamp without time zone, shift varchar,
pasien varchar, total double precision, tunai double precision, kredit double precision)
group by tanggal::date, shift
order by tanggal::date, case when shift='P' then 0 else 1 end]]>
	</queryString>
